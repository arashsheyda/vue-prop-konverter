{
  "name": "vue-prop-konverter",
  "displayName": "Vue Prop Konverter",
  "icon": "assets/icon.png",
  "description": "Converts Vue defineProps({}) object syntax to TypeScript-safe defineProps<>() syntax with destructuring.",
  "publisher": "arashsheyda",
  "version": "0.0.17",
  "repository": {
    "url": "https://github.com/arashsheyda/vue-prop-konverter",
    "type": "git"
  },
  "packageManager": "pnpm@10.18.3",
  "engines": {
    "node": ">=22.20.0",
    "pnpm": ">=10.18.3",
    "vscode": ">=1.106.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:vue"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "vue",
        "extensions": [
          ".vue"
        ]
      }
    ],
    "commands": [
      {
        "command": "vue-prop-konverter.convertProps",
        "title": "Convert defineProps to Type-safe syntax"
      }
    ]
  },
  "scripts": {
    "dev": "vite build --watch --mode development",
    "watch": "vite build --watch",
    "build": "vite build --mode production",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "package": "pnpm exec vsce package --no-dependencies",
    "publish": "pnpm exec vsce publish --no-dependencies",
    "lint": "eslint",
    "lint:fix": "eslint --fix"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.0",
    "@types/babel__generator": "^7.27.0",
    "@types/babel__traverse": "^7.28.0",
    "@types/node": "^24.10.1",
    "@types/vscode": "^1.106.0",
    "@vitest/ui": "4.0.9",
    "@vscode/vsce": "^3.7.0",
    "eslint": "^9.39.1",
    "semantic-release": "^25.0.0",
    "typescript": "^5.4.0",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.2",
    "vitest": "^4.0.9"
  },
  "dependencies": {
    "@babel/generator": "^7.28.5",
    "@babel/parser": "^7.28.5",
    "@babel/traverse": "^7.28.5",
    "@babel/types": "^7.28.5",
    "@vue/compiler-sfc": "^3.5.24"
  }
}
